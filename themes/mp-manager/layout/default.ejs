    <script src="/assets/mp-manager/js/custom/resource.js" type="text/javascript"></script>
    <script src="//tinymce.cachefly.net/4.1/tinymce.min.js" type="text/javascript"></script>
    <script src="http://zurb.com/playground/uploads/upload/upload/235/ajaxupload.js" type="text/javascript"></script>
  </head>
  <body>
    <header>
        <div class="top-sec">
            <div class="container">
                <ul class=" top-nav pull-left">
                  <li class="active"><a href="/"><i class="glyphicon glyphicon-chevron-left"></i> Back to Website</a></li>
                </ul>
                <%	if(true === isLoggedin)	{	%>
	                <ul class=" top-nav pull-right">
	                  <li class="active"><a href="/manager/profile"><i class="glyphicon glyphicon-cog"></i> admin@techunits.com</a></li>
	                  <li class="active"><a href="/mp-manager/logout"><i class="glyphicon glyphicon-share-alt"></i> Logout</a></li>
	                </ul>
	            <%	}	%>
            </div>
        </div>
    </header>
    <section class="container">
